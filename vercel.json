{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/static/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
